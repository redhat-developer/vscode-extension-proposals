{
	"compilerOptions": {
		"target": "ES2020",
		"lib": [
			"ES2020",
		],
		"declaration": true,
		"declarationMap": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./lib"
	},
}